{
    "name": "txt2tags",
    "displayName": "txt2tags",
    "version": "0.0.1",
    "publisher": "aureliojargas",
    "description": "txt2tags language support for Visual Studio Code",
    "author": {
        "name": "Aurelio Jargas"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/txt2tags/vscode.git"
    },
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "txt2tags",
                "aliases": [
                    "Txt2tags",
                    "txt2tags"
                ],
                "extensions": [
                    ".t2t"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "txt2tags",
                "scopeName": "text.txt2tags",
                "path": "./syntaxes/txt2tags.tmLanguage"
            }
        ]
    }
}
